{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
        "value": "eastus"
    },
    "environment": {
        "value": "dev"
    },

    "containerNames": {
        "value": [
          "bronze",
          "silver",
          "gold"
        ]
    },
    "storageConfig": {
      "value": {
        "kind": "StorageV2",
        "sku_name": "Standard_LRS",
        "allowBlobPublicAccess": true,
        "isHnsEnabled": true,
        "accessTier": "Hot"
      }
    },
    "resourceGroupName" :{
      "value": "synapse-dev-rg"
    },
    "workspaceName" : {
      "value": "synapsewsdev"
    },
    "pricingTier": {
      "value": "premium"
    },
    "ShouldCreateContainers": {
      "value": true
    },
    "loganalyticswsname": {
      "value": "loganalyticsdev"
    },
    "appInsightswsname": {
      "value": "appinsightsdev"
    },
    "storageAccountName": {
      "value": "adlsdev"
    }
  }
}
  
